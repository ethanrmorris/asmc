<%- include("partials/headers/main") %>

<div class="container">
  <div class="container-fluid">
    <img src="/images/news_banner.png" alt="" class="banner" />
  </div>

  <% articles.forEach(function(articles){ %>
  <div class="container-fluid news-parent no-padding-award">
    <div>
      <a href="/article5">
        <div class="row news-margin">
          <div class="col-md-6 no-padding-award">
            <img
              src="/images/<%=article.image%>.png"
              alt="<%=article.title%>"
              class="news-pic"
            />
          </div>
          <div class="col-md-6 no-padding-award blog-caption">
            <h3><%=article.title%></h3>
            <br />
            <br />
            <p><%=article.time%></p>
          </div>
        </div>
      </a>
    </div>
  </div>
  <% }) %>
</div>

<%- include("partials/footer") %>
