            <tr class="career-stats">
              <td>Career</td>
              <td class="center-data"><%= stats[4].average.oGP %></td>
              <td class="center-data"><%= stats[4].average.oW %></td>
              <td class="center-data"><%= stats[4].average.oL %></td>
