  <div class="container-fluid">
    <img src="/images/ethan/banner.png" alt="" class="banner" />
  </div>

<!-- Owner Box/Logo -->

  <div class="container-fluid row box-border">
    <div class="col-md-12 left-data ethan-team-header">
      <h1 class="team-name">SILVERBACKS</h1>
      <h5>Ethan Morris, <em>Owner</em></h5>
    </div>
    <div class="col-md-6 left-data">
      <div class="top-bottom-padding">
        <p><strong>Height:</strong> 6'4"</p>
        <p><strong>Weight:</strong> 270 lbs.</p>
        <p><strong>College:</strong> University of Illinois-Springfield</p>
        <p><strong>Favorite Team:</strong> Indianapolis Colts</p>
      </div>
      <div>
        <h6 class="table-title quick-view">
          <span class="ethan-table-title-fill">Quick View</span>
        </h6>
      </div>
      <div class="table-responsive">
        <table class="table table-hover standard-stats quick-stats draft-table">
          <thead>
            <tr class="center-data">
              <th>Year</th>
              <th><abbr title="Games Played">GP</abbr></th>
              <th><abbr title="Wins">W</abbr></th>
              <th><abbr title="Losses">L</abbr></th>
              <th><abbr title="Ties">T</abbr></th>
              <th><abbr title="Winning Percentage">Pct.</abbr></th>
              <th><abbr title="Points For">PF</abbr></th>
              <th><abbr title="Points Against">PA</abbr></th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td><%= stats[4].year[0].year %></td>
              <td class="center-data"><%= stats[4].year[0].oGP %></td>
              <td class="center-data"><%= stats[4].year[0].oW %></td>
              <td class="center-data"><%= stats[4].year[0].oL %></td>
              <td class="center-data"><%= stats[4].year[0].oT %></td>
              <td class="right-data"><%= stats[4].year[0].oPct %></td>
              <td class="right-data"><%= stats[4].year[0].oPF %></td>
              <td class="right-data"><%= stats[4].year[0].oPA %></td>
